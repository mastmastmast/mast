/*综合应用*/
.score-outer{
  float: left;
  width: 50%;
  text-align: center;
  font-size: 28px;
  margin-top: 160px;
}
/*.score-outer span {*/
  /*color: #ff640f;*/
/*}*/
.score-bar{
  position: relative;
  margin-top: 160px;
}
.score-bar ul{
  width: 400px;
  overflow: hidden;
  margin: 0 auto;
}
.score-bar ul .single-bar{
  width: 80px;
  float: left;
  font-size: 18px;
  line-height: 44px;
  text-align: center;
  border-bottom: 6px solid red;
}
.score-bar ul .single-bar span{
  display: inline-block;
  line-height: 24px;
  border-radius: 2px;
  padding: 0px 5px;
}
.score-bar ul .single-bar:nth-child(2){
  border-bottom: 6px solid #ff9900;
}
.score-bar ul .single-bar:nth-child(3){
  border-bottom: 6px solid #009933;
}
.score-bar ul .single-bar:nth-child(4){
  border-bottom: 6px solid #08c9ac;
}
.score-bar ul .single-bar:nth-child(5){
  border-bottom: 6px solid #0099ff;
}
.score-bar .selected{
  color: white;
}
.score-bar ul .single-bar:nth-child(1) .selected{
  background: red;
  position: relative;
}

.score-bar ul .single-bar:nth-child(1) .selected::after {
  content: "";
  width: 0px;
  height: 0px;
  border-width: 8px 5px 0px;
  border-style: solid;
  border-color: red transparent transparent transparent;
  position: absolute;
  top: 22px;
  left: 18px;
}

.score-bar ul .single-bar:nth-child(2) .selected{
  position: relative;
  background: #ff9900;
}

.score-bar ul .single-bar:nth-child(2) .selected::after {
  content: "";
  width: 0px;
  height: 0px;
  border-width: 8px 5px 0px;
  border-style: solid;
  border-color: #ff9900 transparent transparent transparent;
  position: absolute;
  top: 22px;
  left: 18px;
}

.score-bar ul .single-bar:nth-child(3)  .selected{
  background: #009933;
  position: relative;
}

.score-bar ul .single-bar:nth-child(3) .selected::after {
  content: "";
  width: 0px;
  height: 0px;
  border-width: 8px 5px 0px;
  border-style: solid;
  border-color: #009933 transparent transparent transparent;
  position: absolute;
  top: 22px;
  left: 18px;
}

.score-bar ul .single-bar:nth-child(4) .selected{
  background: #08c9ac;
  position: relative;
}

.score-bar ul .single-bar:nth-child(4) .selected::after {
  content: "";
  width: 0px;
  height: 0px;
  border-width: 8px 5px 0px;
  border-style: solid;
  border-color: #08c9ac transparent transparent transparent;
  position: absolute;
  top: 22px;
  left: 18px;
}

.score-bar ul .single-bar:nth-child(5) .selected{
  background: #0099ff;
  position: relative;
}

.score-bar ul .single-bar:nth-child(5) .selected::after {
  content: "";
  width: 0px;
  height: 0px;
  border-width: 8px 5px 0px;
  border-style: solid;
  border-color: #0099ff transparent transparent transparent;
  position: absolute;
  top: 22px;
  left: 18px;
}

