.underline {
  display: inline-block;
  box-sizing: border-box;
  min-width: 120px;
  height: 45px;
  position: relative;
  border-bottom: 1px solid #333333;
  text-indent: 0;
  text-align: center;
  vertical-align: top;
}

.underline-cu {
  display: inline-block;
  box-sizing: border-box;
  min-width: 120px;
  height: 50px;
  position: relative;
  border-bottom: 2px solid #333333;
  text-indent: 0;
  text-align: center;
  vertical-align: top;
}

.underline-cu2 {
  box-sizing: border-box;
  position: relative;
  border-bottom: 2px solid #333333;
  text-indent: 0;
  text-align: center;
  vertical-align: top;
}

.table-div {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  border-bottom: 2px solid #ccc;
}

.table-div span {
  box-sizing: border-box;
  display: inline-block;
  width: 24.99%;
  height: 90px !important;
  padding: 14px 20px;
  border-right: 2px solid #ccc;
  vertical-align: top;
}

.table-div span:last-of-type {
  border-right: none !important;
}

.table-div .table-div-key {
  background: #f0f0f0;
  font-size: 22px;
  color: #666;
}

.table-div:last-of-type {
  border-bottom: none !important;
}

.table-div .table-div-one {
  line-height: 62px !important;
}

#table2 td.value {
  text-align: left !important;
}

.no-width {
  min-width: 0px;
  height: 35px;
}

.aviods {
  page-break-before: always;
  page-break-after: always;
  page-break-inside: avoid !important;
}
