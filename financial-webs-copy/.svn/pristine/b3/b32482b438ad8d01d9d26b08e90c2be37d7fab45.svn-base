<script type="text/html" id="loan-review-tp">
  <div class="content-box">
    <h2 class="content-box-title">2.1 技术指标情况</h2>
    <h3 class="content-box-title">2.1.1 知识产权/标准情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          <td class="text-left">
            {{if technical.intellectualProps && technical.intellectualProps.length>0}}
            {{each technical.intellectualProps item1}}
            <span class="pdf-checkbox short {{item1.exist ? 'checked':''}}">{{item1.codeName}}</span>
            {{/each}}
            {{else}}
            暂无数据
            {{/if}}
          </td>
        </tr>
        {{if technical.intellectualDesc && technical.intellectualDesc.length > 0}}
        {{each technical.intellectualDesc value}}
        <tr>
          <td>
            <ul class="table-ul">
              {{each value.desc value2}}
              <li class="table-ul-li">{{value2}}</li>
              {{/each}}
            </ul>
          </td>
        </tr>
        {{/each}}
        {{else}}
        <tr>
          <td>暂无数据</td>
        </tr>
        {{/if}}
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.1.2 权威认定情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          {{if technical.authIdentificateProps && technical.authIdentificateProps.length>0}}
          <td class="text-left">
            {{each technical.authIdentificateProps item2}}
            <span class="pdf-checkbox short {{item2.exist ? 'checked':''}}">{{item2.codeName}}</span>
            {{/each}}
            {{if technical.authIdentificateOther}}
            <p>其他：</p><p>{{technical.authIdentificateOther}}</p>
            {{/if}}
          </td>
          {{else}}
          <td>暂无数据</td>
          {{/if}}
        </tr>
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.1.3 研发投入情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th colspan="4">详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          <td>研发费用（万元）</td>
          <td>{{technical.yanfafeiyong | filterNumber2}}</td>
          <td>研发人员（人）</td>
          <td>{{technical.yanfarenyuan | filterNumber2}}</td>
        </tr>
        <tr>
          <td>销售收入（万元）</td>
          <td>{{technical.xiaoshoushouru | filterNumber2}}</td>
          <td>总公司人员（人）</td>
          <td>{{technical.gongsizongrenshu | filterNumber2}}</td>
        </tr>
        <tr>
          <td>研发费用/销售收入</td>
          <td>{{technical.yanfafeiyongbili | filterNumber2}}</td>
          <td>研发人员/总公司人员</td>
          <td>{{technical.yanfarenyuanbili | filterNumber2}}</td>
        </tr>
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.1.4 技术应用情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          {{if technical.shichangqianjing}}
          <td style="text-align: left;text-indent: 2em">{{technical.shichangqianjing}}</td>
          {{else}}
          <td>暂无数据</td>
          {{/if}}
        </tr>
        </tbody>
      </table>
    </div>

    <h2 class="content-box-title">2.2 市场指标情况</h2>
    <h3 class="content-box-title">2.2.1 行业情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          {{if market.fazhanqianjing}}
          <td class="text-left text-indent">{{market.fazhanqianjing}}</td>
          {{else}}
          <td>暂无数据</td>
          {{/if}}
        </tr>
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.2.2 产品竞争力情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          {{if market.chanpinjingzhengli}}
          <td class="text-left text-indent">{{market.chanpinjingzhengli}}</td>
          {{else}}
          <td>暂无数据</td>
          {{/if}}
        </tr>
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.2.3 高管团队</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th width="20%">姓名</th>
          <th width="80%">简介</th>
        </tr>
        </thead>
        <tbody>
          {{if market.leaderDesc && market.leaderDesc.length>0}}
          {{each market.leaderDesc value3}}
          <tr>
            <td>{{value3.name}}</td>
            <td>
              <ul class="table-ul">
                {{each value3.desc value4}}
                <li class="table-ul-li text-indent">{{value4}}</li>
                {{/each}}
              </ul>
            </td>
          </tr>
          {{/each}}
          {{else}}
          <tr>
            <td colspan="2">暂无数据</td>
          </tr>
          {{/if}}
        </tbody>
      </table>
    </div>

    <h3 class="content-box-title">2.2.4 市场拓展情况</h3>
    <div class="content-box-body">
      <table class="pdf-table">
        <thead>
        <tr>
          <th>详情介绍</th>
        </tr>
        </thead>
        <tbody>
        <tr>
          {{if market.tuozhanqianjing}}
          <td class="text-left text-indent">{{market.tuozhanqianjing}}</td>
          {{else}}
          <td>暂无数据</td>
          {{/if}}
        </tr>
        </tbody>
      </table>
    </div>

    <h2 class="content-box-title">2.3 党建信息</h2>
    <div class="content-box-body">
      <table class="pdf-table">
        <tr>
          <td class="cell fill" width="35%">党员人数</td>
          <td class="cell">{{party.party_num | filterNumber2}}</td>
          <td class="cell fill" width="35%">法定代表人是否党员</td>
          <td class="cell">{{party.is_party | filterNumber2}}</td>
        </tr>
        <tr>
          <td class="cell fill" width="35%">高管人数</td>
          <td class="cell">{{party.lead_num | filterNumber2}}</td>
          <td class="cell fill" width="35%">去年党费总额</td>
          <td class="cell">{{party.last_party_cost | filterNumber2}}</td>
        </tr>
        <tr>
          <td class="cell fill" width="35%">是否愿意设立党支部</td>
          <td class="cell">{{party.will_set_party || '-'}}</td>
          <td class="cell fill" width="35%">高管党员人数</td>
          <td class="cell">{{party.lead_party_num | filterNumber2}}</td>
        </tr>
        <tr>
          <td class="cell fill" width="35%">是否设立党支部</td>
          <td class="cell">{{party.is_party_branch || '-'}}</td>
          <td class="cell fill"> </td>
          <td class="cell"> </td>
        </tr>
      </table>
    </div>
  </div>
</script>
