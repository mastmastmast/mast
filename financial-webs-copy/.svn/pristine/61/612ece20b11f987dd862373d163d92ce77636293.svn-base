      'pdf': '../js/pdf',
      'verticalTable': '../js/verticalTable',
      'tableTp': '../template/vertical-table-template.html'
    }
  });
  var resources = [
    'jquery',
    'art-template',
    'echarts',
    'pdf',
    'verticalTable'
  ];
  require(resources, function ($) {
    var pdf = require('pdf');
    var tableData1 = [
      {
        label: '员工总数',
        value: '108人'
      },
      {
        label: '管理人员人数占比',
        value: '7.12%'
      },
      {
        label: '社保参保人数占比',
        value: '95.33%'
      },
      {
        label: '管理人员硕士学历以上人数占比',
        value: '30%'
      }
    ];
    var verticalTable = require('verticalTable');
    var html1 = new verticalTable({
      dataItemCountPerRow: 2,
      labelWidth: '68%',
      data: tableData1
    }).getHtml();
    $('#table1').html(html1);
    var html2 = new verticalTable({
      dataItemCountPerRow: 2,
      labelWidth: '68%',
      data: tableData1,
      title: '信用加分'
    }).getHtml();
    $('#table2').html(html2);
  });
</script>
