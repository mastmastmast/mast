.vertical-table.pdf {
  width: 100%;
  font-size:24px;
  table-layout: fixed;
  border: 2px solid #ccc;
}

.vertical-table.pdf .vertical-table-row {
  page-break-before: always;
  page-break-after: always;
  page-break-inside: avoid;
  border-bottom: 2px solid #ccc;
}

.vertical-table.pdf .vertical-table-row:last-child {
  border-bottom: none;
}

.vertical-table.pdf .vertical-table-row > .cell {
  position: relative;
  line-height: 1.5em;
  padding: 14px 10px;
  color: #333;
  font-weight:bolder;
  border-right: 2px solid #ccc;
  text-align: center;
}
.vertical-table.pdf .vertical-table-row > .cell.label{
  color:#666;
}

.vertical-table.pdf .vertical-table-row > .cell:last-child {
  border-right: none;
}

.vertical-table.pdf .vertical-table-row > .cell.label.fill,
.vertical-table.pdf .vertical-table-row > .cell.title.fill {
  background: rgba(219,219,219,0.3);
  /*box-sizing: border-box;*/
}

.vertical-table.pdf .vertical-table-row > .cell.title {
  padding: 21px 10px;
  font-size:28px;
  text-align: center;
}

.vertical-table.pdf .line {
  position: absolute;
  top: 0;
  width: 1px;
  height: 100%;
  background: #ccc;
  box-sizing: border-box;
}

.vertical-table.pdf .label-bg {
  position: absolute;
  top: 0;
  height: 100%;
  background: rgba(64, 125, 221, .1);
  border-right: 1px solid #ccc;
  box-sizing: border-box;
}

.table-nodata.pdf {
  height: 200px;
  margin: 35px 0;
  line-height: 200px;
  font-size: 28px;
  color: #666;
  text-align: center;
}
