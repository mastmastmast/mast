    methods: {
      print(){
        // this.getPdf('report','企业公共信用报告');
      },
      /**
       * 获取企业的基本信息
       *
       */
      getBaseInfo() {
        enterpriseCredit.getBaseInfo(this.companyName).then(res => {
          if (res.code === '200') {
            this.baseInfo = res.data;
          } else {
            dialog.error(res.message);
          }
        });
      },
      /**
       *雷达图
       */
      drawRadar() {
        let el = document.getElementById('radar-chart');
        enterpriseCredit.getEvaluationInfo(this.companyName).then(res => {
          if (res.code === '200') {
            // console.log(123)
            let indicator = [];
            let data = res.data;
            if (!arrayFunc.isEmpty(data.name) && !arrayFunc.isEmpty(data.value)) {
              $(el).removeClass('nodata');
              data.name.forEach((item, index) => {
                indicator.push({
                  name: item
                });
              });
              let minAndMax = getMinAndMax(data.value);
              let max = minAndMax.max;
              let min = minAndMax.min;
              indicator.forEach((item, index) => {
                item.max = max;
                item.min = min;
              });
              // console.log(indicator)
              ycChart.renderRadarForPdf(el, indicator, data.value, '企业信用评价')
            } else {
              $(el).addClass('nodata')
            }
          } else {
            dialog.error(res.message);
          }
        });
      },
      /**
       * 获取企业的工商信息
       */
      getCommercialInfo() {
        enterpriseCredit.qualification.getCommercialInfo(this.companyName).then(res => {
          if (res.code === '200') {
            this.commercialInfo = res.data;
          } else {
            dialog.error(res.message);
          }
        });
      },
    }
